                    },

                    "visible": {
                        "DEFAULT": false,
                        "US:VIC": true
                    }
                },

                {
                    "template": "teaser-content-basic",
                    "headline": "Springbrook National Park",
                    "copy": "The hour and a half drive from the Brisbane CBD, travels through spectacular rainforest hinterland scenery and ends up at Springbrook National Park, which forms part of world heritage listed Gondwana Rainforest.",

                    "image": {
                        "desktop": "/m7/m7-qld-springbrook-national-park-desktop.jpg",
                        "mobile": "/m7/m7-qld-springbrook-national-park-mobile.jpg"
                    },

                    "cta": {
